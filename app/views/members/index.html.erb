<% if @members.any? %>
<% @members.each do |member| %>
  <div class="card" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title"><%= member.name%></h5>
      <p class="card-text"><%= member.original_url %></p>
    </div>
  </div>
<% end %>
<% else %>
  No Members
<% end %>
