<main>
<% if @members.any? %>
  <div class="container-fluid">
    <div class="row">
      <% @members.each do |member| %>
      <div class="col-sm-6">
        <div class="card">
          <div class="card-body">
            <h5 class="card-title"><%= member.name %></h5>
            <p class="card-text"><%= member.original_url %></p>
            <p class="card-text"><%= member.shortened_url %></p>
          </div>
        </div>
      </div>
      <% end %>
    </div>
  </div>
<% else %>
  No Members
<% end %>
</main>